set terminal svg
set output "fig1.svg"
set xlabel "x"
set ylabel "y"
set key right bottom
set tics out
plot [-2:2] \
	"erf.data" using 1:2 with points pointtype 4 title "erf from cmath", \
	erf(x) with lines title "erf(x) in gnuplot", \
	"erf_tabulated.txt" using 1:2 with points pointtype 7 title "erf from wiki", \
	"erf.data" using 1:3 with points pointtype 2 title "erf approx func"	